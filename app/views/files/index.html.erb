
<div class="container">
  <h1>Files</h1>
  <table class="table">
      <tr>
          <th>Files</th>
          <th>Actions</th>
      </tr>
    <% @files.each do |file| %>
        <tr>
            <td><%= file %></td>
            <td><%= link_to 'view',file_path(file)%></td>
        </tr>
    <% end %>
  </table>
</div>
